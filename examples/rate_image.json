{
   "src": {
        "config": {
            "sources": [
                ["cspad", [512, 512]]
            ],
            "pedestal": 32,
            "width": 5,
            "outputs": [
                ["sender", "data", "cspad"],
                ["source", "null", "cspad"]
            ]
        },
        "optype": "FixedSrc"
    },
    "gather": {
        "config": {
            "num_average": 20,
            "outputs": [
                ["counter", "null", "cspad_data"]
            ],
            "port": 5557
        },
        "optype": "Reduce"
    },
    "sender": {
        "config": {
            "host": "localhost",
            "port": 5557,
            "topic": "cspad_data"
        },
        "optype": "Send"
    },
    "counter": {
        "config": {
            "name": "gather",
            "rate": 2,
            "outputs": [
                ["myimage", "image"]
            ]
        },
        "optype": "Rate"
    },
    "myimage": {
        "config": {
            "title": "A test image",
            "rate": 0.1
        },
        "optype": "Image"
    },
    "source": {
        "config": {
            "name": "source",
            "rate": 2,
            "outputs": [
            ]
        },
        "optype": "Rate"
    }
}
