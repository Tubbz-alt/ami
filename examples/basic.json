        "config": {
            "imports": ["numpy"],
            "expression": "numpy.sum(cspad, axis=1)"
        },
        "inputs": [{"name": "cspad", "required": true}],
        "optype": "Eval"
    },
    "cspad-sum": {
        "config": {
            "imports": ["numpy"],
            "expression": "numpy.sum(cspad)"
        },
        "inputs": [{"name": "cspad", "required": true}],
        "optype": "Eval"
    },
    "gasdet-inverse": {
        "config": {
            "expression": "gasdet * -1"
        },
        "inputs": [{"name": "gasdet", "required": true}],
        "optype": "Eval"
    }
